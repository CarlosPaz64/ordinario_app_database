//- _dashboard.pug
section.dashboard
  .top
    i(class="uil uil-bars sidebar-toggle")
  .dash-content
    .overview
      .title
        i(class="uil uil-tachometer-fast-alt")
        span.text Â¡Hola #{user.nombre}! Bienvenido al dashboard.
      .boxes
        .box.box1
          i(class="uil uil-thumbs-up")
          span.text Total Likes
          span.number 50,120
        .box.box2
          i(class="uil uil-comments")
          span.text Comments
          span.number 20,120
        .box.box3
          i(class="uil uil-share")
          span.text Total Share
          span.number 10,120
    .activity
